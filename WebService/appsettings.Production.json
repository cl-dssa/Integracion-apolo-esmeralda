{
  "Serilog":{
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Error",
        "Microsoft.EntityFrameworkCore.Database.Command": "Error"
      }
    },
    "WriteTo":[
      {
        "Name": "Console",
        "Args":{
          "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithExceptionDetails" ]
  },
  "urlApiMinsal": "", 
  "urlEsmeralda": "" 
}


